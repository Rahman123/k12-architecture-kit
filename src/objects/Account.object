    <fields>
        <fullName>High_School__c</fullName>
        <defaultValue>false</defaultValue>
        <description>When check, signifies that this school organization handles high school grades. A school can belong to more than one of these categories such as 6th through 12th grads would be High School AND Middle School</description>
        <externalId>false</externalId>
        <label>High School</label>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Local_Education_Agency_Id__c</fullName>
