        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Perpetrator</fullName>
                    <default>false</default>
                    <label>Perpetrator</label>
                </value>
                <value>
                    <fullName>Reporter</fullName>
                    <default>false</default>
                    <label>Reporter</label>
                </value>
