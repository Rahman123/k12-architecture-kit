    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <fields>
        <fullName>Incident__c</fullName>
        <description>This is the &quot;master&quot; case that captured the incident details in which multiple students are involved. The thought being that if there were different remediation cases/interventions that needed to take place they could be Child Cases of this case as well.</description>
        <externalId>false</externalId>
        <inlineHelpText>Please choose the incident for which this student was involved.</inlineHelpText>
        <label>Incident</label>
        <referenceTo>Case</referenceTo>
        <relationshipLabel>Parties Involved</relationshipLabel>
        <relationshipName>Incident_Parties</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Involved_Party__c</fullName>
        <description>The student/teacher/other contact that was involved in the given incident.  Using the term &quot;Party&quot; here since potentially this could involved a teacher, counselor, etc. so Student was not an ideal choice of term.</description>
        <externalId>false</externalId>
        <label>Involved Party</label>
        <referenceTo>Contact</referenceTo>
        <relationshipLabel>Disciplinary Incidents</relationshipLabel>
        <relationshipName>Disciplinary_Incidents</relationshipName>
        <relationshipOrder>1</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Role__c</fullName>
        <description>This field represents the role in which a particular party involved in an incident has played.</description>
        <externalId>false</externalId>
        <inlineHelpText>Please choose the extent to which this party was involved.</inlineHelpText>
        <label>Role</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
