        <description>measures the fluency of the language spoken. EdFi specifically calls out &quot;Limited/Not Limited&quot; so those are provided here.</description>
        <externalId>false</externalId>
        <label>Fluency</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
